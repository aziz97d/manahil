@model List<manahil.ViewModels.DashboardProjectsViewModels>
@{
    Layout = null;
}

<form class="card p-2 table-responsive">
    <table id="tbllist" class="table table-striped table-bordered table-sm text-nowrap table-hover" cellspacing="0" width="100%">
        <thead class="">
            <tr>
                <th>Manahil Sl</th>
                <th>Donor</th>
                <th>Donor Sl</th>
                <th>Category</th>
                <th>Contractor</th>
                <th>Employee</th>
                <th>Tamid</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.ManahilSerial)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DonorName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.DonorSerial)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Category)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.ContractorName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.EmployeeName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.TamidNumber)
                    </td>
                </tr>
            }
        </tbody>
    </table>

</form>
